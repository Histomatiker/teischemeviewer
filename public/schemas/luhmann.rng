								<choice>
									<ref name="model.global"/>
									<ref name="model.divGenLike"/>
								</choice>
							</zeroOrMore>
						</group>
					</oneOrMore>
					<oneOrMore>
						<group>
							<ref name="model.div1Like"/>
							<zeroOrMore>
								<choice>
									<ref name="model.global"/>
									<ref name="model.divGenLike"/>
								</choice>
							</zeroOrMore>
						</group>
					</oneOrMore>
					<group>
						<oneOrMore>
							<group>
								<ref name="model.common"/>
								<zeroOrMore>
									<ref name="model.global"/>
								</zeroOrMore>
							</group>
						</oneOrMore>
						<optional>
							<choice>
								<oneOrMore>
									<group>
										<ref name="model.divLike"/>
										<zeroOrMore>
											<choice>
												<ref name="model.global"/>
												<ref name="model.divGenLike"/>
											</choice>
										</zeroOrMore>
									</group>
								</oneOrMore>
